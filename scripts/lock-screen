#!/bin/bash
i3lock -n -i $(eval "set -- $(sed 1d "$HOME/.fehbg")"; echo $4) --bar-indicator --bar-color=$(xrdb -get color0 | sed -e "s/#//") -k --keyhlcolor=$(xrdb -get color1 | sed -e "s/#//") --verifcolor=$(xrdb -get color1 | sed -e "s/#//") -C --bshlcolor=$(xrdb -get color3 | sed -e "s/#//") --wrongcolor=$(xrdb -get color3 | sed -e "s/#//") --timecolor=$(xrdb -get color7 | sed -e "s/#//")  --datecolor=$(xrdb -get color7 | sed -e "s/#//")
wal -i $(eval "set -- $(sed 1d "$HOME/.fehbg")"; echo $4)